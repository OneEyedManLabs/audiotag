<?xml version="1.0" encoding="utf-8"?>
<!--
   AudioTag backup rules for Android Auto Backup
   Conservative approach: only backup essential app data
   Large audio files require manual export
-->
<full-backup-content>
    <!-- Include app preferences/settings -->
    <include domain="sharedpref" path="."/>
    
    <!-- Include database files -->
    <include domain="database" path="."/>
    
    <!-- Exclude all external files - use manual export instead -->
    <exclude domain="external" path="."/>
</full-backup-content>